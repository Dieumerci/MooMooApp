{
    "extends": "airbnb/legacy",
    "plugins": [
        "react"
    ],
    "ecmaFeatures": {
        "jsx": true
    },
    "env": {
    "browser": true,
    "node": true
    },
    "rules": {
      "strict": 0,
      "no-underscore-dangle": 0,
      "quotes": [2, "single"],
      "no-shadow": 2,
      "one-var": 0,
      "comma-dangle": [2,"never"],
      "func-names":0
    }

}
